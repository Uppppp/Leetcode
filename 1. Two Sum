class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
        remainders = dict()
        
        for i in range(len(nums)):
            if nums[i] in remainders:
                return [remainders[nums[i]], i]
            else:
                remainders[target-nums[i]] = i
