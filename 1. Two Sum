class Solution:
    def twoSum(self, nums, target):
        number = []
        for number_1 in range(0,len(nums)):
            for number_2 in range((number_1+1),len(nums)):
                if(nums[number_1] + nums[number_2]) == target:
                    number.append(number_1)
                    number.append(number_2)
        return number

